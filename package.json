{
  "name": "simon-game",
  "version": "0.2.0",
  "description": "A React Simon Game",
  "author": "Juan Riquelme <juanriqgon@protonmail.com>",
  "neutrino": {
    "config": {
      "entry": {
        "vendor": [
          "animejs",
          "lodash.debounce",
          "react",
          "react-dom",
          "react-redux",
          "react-router",
          "react-router-dom",
          "redux",
          "redux-thunk",
          "styled-jsx/style"
        ]
      }
    },
    "use": [
      "neutrino-preset-react",
      "neutrino-preset-airbnb",
      "custom_presets/styled-jsx-preset.js",
      "custom_presets/set-eslint-envs.js"
    ]
  },
  "scripts": {
    "start": "neutrino start",
    "build": "neutrino build",
    "deploy": "surge --project ./build --domain reactsimon.surge.sh"
  },
  "devDependencies": {
    "neutrino": "^5.4.0",
    "neutrino-preset-airbnb": "^2.1.0",
    "neutrino-preset-react": "^5.4.0"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "lodash.debounce": "^4.0.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-jsx": "^0.5.7"
  }
}
